{
    "name": "eberkund/ranger",
    "version": "0.1",
    "description": "Easily expand text to an array of integers",
    "license": "MIT",
    "type": "library",
    "support": {
        "issues": "https://github.com/eberkund/ranger/issues",
        "source": "https://github.com/eberkund/ranger"
    },
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.14.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Eberkund\\Range\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
